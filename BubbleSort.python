def bubble_sort(arr):
    n = len(arr)
    for i in range(n - 1):
        for j in range(n - i - 1):
            if arr[j] > arr[j + 1]:
                arr[j], arr[j + 1] = arr[j + 1], arr[j]

if __name__ == "__main__":
    n = int(input("Enter the number of elements: "))
    arr = []
    for i in range(n):
        num = int(input("Enter the number: "))
        arr.append(num)

    bubble_sort(arr)
    
    print("Sorted array:", arr)






